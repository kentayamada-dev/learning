{
	"name": "${containerWorkspaceFolderBasename}",
	"build": {
		"dockerfile": "Dockerfile",
		"args": {
			"WORKSPACE_PATH": "${containerWorkspaceFolder}"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"Fooxly.themeswitch",
				"albert.TabOut",
				"richie5um2.vscode-sort-json",
				"onatm.open-in-new-window"
			]
		}
	}
}
